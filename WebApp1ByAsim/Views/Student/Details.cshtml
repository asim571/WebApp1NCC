@model WebApp1ByAsim.Models.Student

<h1>Student Details</h1>

<table class="table">
    <tr>
        <th>Student ID</th>
        <td>@Model.StdID</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Faculty</th>
        <td>@Model.Faculty</td>
    </tr>
</table>
